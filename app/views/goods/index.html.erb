<h1>Treasures of the Seven Kingdoms</h1>
<div class="grid-container">
    <% @goods.each do |good| %>
  <div class="card grid-item">
      <%= cl_image_tag good.photo.key, height: 300, width: 400, crop: :fill %>
      <li><%= link_to good.name, good_path(good) %></li>
  </div>
    <% end %>
</div>

<div class="container">
  <div class="btn btn-warning">
  <%= link_to "Add a Relic!", new_good_path %>
  </div>
  <%= link_to "My Inventory!", dashboard_path %>
</div>